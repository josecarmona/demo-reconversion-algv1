<?xml version="1.0" encoding="utf-8"?>
<odoo noupdate="0">

    <record id="sequence_advance_payment_supplier" model="ir.sequence">
        <field name="name">Advance Payment Supplier</field>
        <field name="code">advance.payment.supplier</field>
        <field name="prefix">ADV.OUT/%(range_year)s/</field>
        <field eval="1" name="number_next"/>
        <field eval="1" name="number_increment"/>
        <field eval="True" name="use_date_range"/>
        <field eval="False" name="company_id"/>
        <field name="padding">4</field>
    </record>

    <record id="sequence_advance_payment_customer" model="ir.sequence">
        <field name="name">Advance Payment Customer</field>
        <field name="code">advance.payment.customer</field>
        <field name="prefix">ADV.IN/%(range_year)s/</field>
        <field eval="1" name="number_next"/>
        <field eval="1" name="number_increment"/>
        <field eval="True" name="use_date_range"/>
        <field eval="False" name="company_id"/>
        <field name="padding">4</field>
    </record>

</odoo>
